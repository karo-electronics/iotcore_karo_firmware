	tmu@0x30260000 {
		compatible = "fsl,imx8mm-tmu";
		reg = <0x0 0x30260000 0x0 0x10000>;
		clocks = <0x3 0x1b6>;
		interrupt = <0x0 0x31 0x4>;
		#thermal-sensor-cells = <0x0>;
		linux,phandle = <0xd>;
		phandle = <0xd>;
