		#gpio-cells = <0x2>;
		interrupt-controller;
		#interrupt-cells = <0x2>;
		linux,phandle = <0x37>;
		phandle = <0x37>;
	};

	gpio@30230000 {
		compatible = "fsl,imx8mm-gpio", "fsl,imx35-gpio";
