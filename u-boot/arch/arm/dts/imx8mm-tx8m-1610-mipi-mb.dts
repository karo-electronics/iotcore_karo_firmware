	};

	caam@30900000 {
		compatible = "fsl,sec-v4.0";
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		reg = <0x0 0x30900000 0x0 0x40000>;
		ranges = <0x0 0x0 0x30900000 0x40000>;
		interrupts = <0x0 0x5b 0x4>;

		jr0@1000 {
			compatible = "fsl,sec-v4.0-job-ring";
			reg = <0x1000 0x1000>;
			interrupts = <0x0 0x69 0x4>;
		};

		jr1@2000 {
			compatible = "fsl,sec-v4.0-job-ring";
			reg = <0x2000 0x1000>;
			interrupts = <0x0 0x6a 0x4>;
		};

		jr2@3000 {
